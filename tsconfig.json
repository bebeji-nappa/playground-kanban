{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2020"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "baseUrl": "/",
  "paths": {
    "@/*": "app/frontend/*",
  },
  "include": [
    "app/frontend",
  ],
  "exclude": [
    "node_modules"
  ],
}
